<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:language="en"
          jcr:mixinTypes="[mix:language]"
          jcr:primaryType="sling:Folder">
    <TRACKS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="TRACKS"
        sling:message="My Tracks"/>
    <ALBUMS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="ALBUMS"
        sling:message="My Albums"/>
    <SEARCH
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="SEARCH"
        sling:message="Search Spotify"/>
    <TAB-TRACKS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="TAB-TRACKS"
        sling:message="Tracks"/>
    <TAB-ALBUMS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="TAB-ALBUMS"
        sling:message="Albums"/>
    <SEARCH-ALBUMS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="SEARCH-ALBUMS"
        sling:message="search albums..."/>
    <SEARCH-TRACKS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="SEARCH-TRACKS"
        sling:message="search tracks..."/>
</jcr:root>
